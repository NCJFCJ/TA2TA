<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

    <!-- adobe_link -->
    <field
      description="COM_SERVICES_FORM_WEBINAR_ADOBE_LINK_DESC"
      label="COM_SERVICES_FORM_WEBINAR_ADOBE_LINK_LBL"
      maxlength="150"
      name="adobe_link"
      type="text" />
    
    <!-- adobe_number -->
    <field
      class="inputbox"
      default="1126783"
      description="COM_SERVICES_FORM_WEBINAR_ADOBE_NUMBER_DESC"
      label="COM_SERVICES_FORM_WEBINAR_ADOBE_NUMBER_LBL"
      name="adobe_number"
      size="1"
      type="list">
        <option value="1126783">Meeting Room - 1126783</option>
        <option value="9998597">Seminar Room - 9998597</option>
    </field>
    
    <!-- alias -->
    <field
      description="COM_SERVICES_FORM_WEBINAR_ALIAS_DESC"
      label="COM_SERVICES_FORM_WEBINAR_ALIAS_LBL"
      maxlength="50"
      name="alias"
      type="text" />

		<!-- checked_out -->
		<field
			filter="unset"
			name="checked_out"
			type="hidden" />

		<!-- checked_out_time -->
    <field
    	filter="unset"
    	name="checked_out_time"
    	type="hidden" />

    <!-- comments -->
    <field 
      cols="8"
      description="COM_SERVICES_FORM_COMMENTS_DESC" 
      filter="safehtml"
      label="COM_SERVICES_FORM_COMMENTS_LBL"
      name="comments"
      rows="4"
      type="textarea" />
	  
	  <!-- created -->
    <field
			name="created"
			type="hidden" />
		
		<!-- created_by -->
   	<field
   		default="" 
      description="COM_SERVICES_FORM_CREATED_BY_DESC"
      label="COM_SERVICES_FORM_CREATED_BY_LBL"
      name="created_by"
      readonly="true"
   		type="user" />
    
    <!-- date -->
    <field
      description="COM_SERVICES_FORM_WEBINAR_DATE_DESC"
      filter="none"
      format="%m/%d/%Y"
      label="COM_SERVICES_FORM_WEBINAR_DATE_LBL"
      name="date"
      type="calendar" />

    <!-- description -->
    <field 
      cols="8"
      description="COM_SERVICES_FORM_WEBINAR_DESCRIPTION_DESC" 
      filter="safehtml"
      label="COM_SERVICES_FORM_WEBINAR_DESCRIPTION_LBL"
      maxlength="500"
      name="description"
      rows="4"
      type="textarea" />

    <!-- end_time -->
    <field
      class="form-control"
      description="COM_SERVICES_FORM_WEBINAR_END_TIME_DESC"
      label="COM_SERVICES_FORM_WEBINAR_END_TIME_LBL"
      name="end_time"
      type="list">
      <option value="8:15am">8:15am</option>
      <option value="8:30am">8:30am</option>
      <option value="8:45am">8:45am</option>
      <option value="9:00am">9:00am</option>
      <option value="9:15am">9:15am</option>
      <option value="9:30am">9:30am</option>
      <option value="9:45am">9:45am</option>
      <option value="10:00am">10:00am</option>
      <option value="10:15am">10:15am</option>
      <option value="10:30am">10:30am</option>
      <option value="10:45am">10:45am</option>
      <option value="11:00am">11:00am</option>
      <option value="11:15am">11:15am</option>
      <option value="11:30am">11:30am</option>
      <option value="11:45am">11:45am</option>
      <option value="12:00pm">12:00pm</option>
      <option value="12:15pm">12:15pm</option>
      <option value="12:30pm">12:30pm</option>
      <option value="12:45pm">12:45pm</option>
      <option value="1:00pm">1:00pm</option>
      <option value="1:15pm">1:15pm</option>
      <option value="1:30pm">1:30pm</option>
      <option value="1:45pm">1:45pm</option>
      <option value="2:00pm">2:00pm</option>
      <option value="2:15pm">2:15pm</option>
      <option value="2:30pm">2:30pm</option>
      <option value="2:45pm">2:45pm</option>
      <option value="3:00pm">3:00pm</option>
      <option value="3:15pm">3:15pm</option>
      <option value="3:30pm">3:30pm</option>
      <option value="3:45pm">3:45pm</option>
      <option value="4:00pm">4:00pm</option>
    </field>

    <!-- features -->
    <field
      cols="2"
      description="COM_SERVICES_FORM_WEBINAR_FEATURES_DESC"
      key_field="id"
      label="COM_SERVICES_FORM_WEBINAR_FEATURES_LBL"
      name="features"
      type="SQLcheckboxes"
      query="SELECT `id`, `state`, `name` FROM `#__services_webinar_features` ORDER BY `name`;"
      value_field="name" />
    
    <!-- file -->
   	<field
   		accept="application/pdf"
   		description="COM_SERVICES_FORM_FILE_DESC"
   		label="COM_SERVICES_FORM_FILE_LBL"
   		name="file"
   		type="file" />
		
		<!-- ID -->
		<field
			class="readonly"
      default="0"
			description="JGLOBAL_FIELD_ID_DESC"
			label="COM_SERVICES_FORM_MEETING_ID_LBL"
      name="id"
			readonly="true"
			type="hidden" /> 
      
    <!-- modified -->
    <field
      name="modified"
      type="hidden" />
		
		<!-- modified_by -->
		<field
			name="modified_by"
			type="hidden" />

    <!-- num_participants -->
    <field 
      description="COM_SERVICES_FORM_WEBINAR_NUM_PARTICIPANTS_DESC" 
      label="COM_SERVICES_FORM_WEBINAR_NUM_PARTICIPANTS_LBL"
      max="999999"
      min="1"
      name="num_participants"
      type="number" />
    
    <!-- org -->
    <field
    	description="COM_SERVICES_FORM_ORG_DESC"
    	disabled_message="COM_SERVICES_FORM_ORG_DISABLED"
    	label="COM_SERVICES_FORM_ORG_LBL"
    	name="org"
    	type="SQLdisabled"
    	query="SELECT `id` as value, `name` as org, `state` FROM #__ta_providers ORDER BY `name`;" />
       
    <!-- project -->
    <field
      class="inputbox"
      default="0"
      description="COM_SERVICES_FORM_PROJECT_DESC"
      label="COM_SERVICES_FORM_PROJECT_LBL"
      name="project"
      size="1"
      type="list">
        <option value="0">-- Select One --</option>
    </field>
    
    <!-- registration -->
    <field
      class="btn-group"
      default="0"
      description="COM_SERVICES_FORM_WEBINAR_REGISTRATION_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_WEBINAR_REGISTRATION_LBL"
      name="registration"
      required="true" 
      type="radio">
        <option value="1">Enabled</option>
        <option value="0">Disabled</option>
    </field>
    
    <!-- registration_adv_accessibility -->
    <field
      class="btn-group"
      default="0"
      description="COM_SERVICES_FORM_WEBINAR_REGISTRATION_ADV_ACCESSIBILITY_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_WEBINAR_REGISTRATION_ADV_ACCESSIBILITY_LBL"
      name="registration_adv_accessibility"
      type="radio">
        <option value="1">Enabled</option>
        <option value="0">Disabled</option>
    </field> 

    <!-- registration_q1-->
    <field
      description="COM_SERVICES_FORM_REGISTRATION_Q_DESC"
      label="COM_SERVICES_FORM_REGISTRATION_Q_LBL"
      maxlength="255"
      name="registration_q1"
      type="text" />

    <!-- registration_q1_type-->
    <field
      default="0"
      description="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_LBL"
      name="registration_q1_type"
      type="list">
        <option value="0">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_FILL_IN</option>
        <option value="1">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_BOOLEAN</option>
    </field> 
    
    <!-- registration_q2-->
    <field
      description="COM_SERVICES_FORM_REGISTRATION_Q_DESC"
      label="COM_SERVICES_FORM_REGISTRATION_Q_LBL"
      maxlength="255"
      name="registration_q2"
      type="text" />

    <!-- registration_q2_type-->
    <field
      default="0"
      description="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_LBL"
      name="registration_q2_type"
      type="list">
        <option value="0">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_FILL_IN</option>
        <option value="1">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_BOOLEAN</option>
    </field> 

    <!-- registration_q3-->
    <field
      description="COM_SERVICES_FORM_REGISTRATION_Q_DESC"
      label="COM_SERVICES_FORM_REGISTRATION_Q_LBL"
      maxlength="255"
      name="registration_q3"
      type="text" />

    <!-- registration_q3_type-->
    <field
      default="0"
      description="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_REGISTRATION_Q_TYPE_LBL"
      name="registration_q3_type"
      type="list">
        <option value="0">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_FILL_IN</option>
        <option value="1">COM_SERVICES_FORM_REGISTRATION_Q_TYPE_BOOLEAN</option>
    </field> 

    <!-- series -->
    <field
      class="btn-group"
      default="0"
      description="COM_SERVICES_FORM_WEBINAR_SERIES_DESC" 
      filter="raw"
      label="COM_SERVICES_FORM_WEBINAR_SERIES_LBL"
      name="series"
      required="true" 
      type="radio">
        <option value="0">Not A Series</option>
        <option value="1">Series</option>
      </field>

    <!-- start_time -->
    <field
      class="form-control"
      description="COM_SERVICES_FORM_WEBINAR_START_TIME_DESC"
      label="COM_SERVICES_FORM_WEBINAR_START_TIME_LBL"
      name="start_time"
      type="list">
      <option value="8:00am">8:00am</option>
      <option value="8:15am">8:15am</option>
      <option value="8:30am">8:30am</option>
      <option value="8:45am">8:45am</option>
      <option value="9:00am">9:00am</option>
      <option value="9:15am">9:15am</option>
      <option value="9:30am">9:30am</option>
      <option value="9:45am">9:45am</option>
      <option value="10:00am">10:00am</option>
      <option value="10:15am">10:15am</option>
      <option value="10:30am">10:30am</option>
      <option value="10:45am">10:45am</option>
      <option value="11:00am">11:00am</option>
      <option value="11:15am">11:15am</option>
      <option value="11:30am">11:30am</option>
      <option value="11:45am">11:45am</option>
      <option value="12:00pm">12:00pm</option>
      <option value="12:15pm">12:15pm</option>
      <option value="12:30pm">12:30pm</option>
      <option value="12:45pm">12:45pm</option>
      <option value="1:00pm">1:00pm</option>
      <option value="1:15pm">1:15pm</option>
      <option value="1:30pm">1:30pm</option>
      <option value="1:45pm">1:45pm</option>
      <option value="2:00pm">2:00pm</option>
      <option value="2:15pm">2:15pm</option>
      <option value="2:30pm">2:30pm</option>
      <option value="2:45pm">2:45pm</option>
      <option value="3:00pm">3:00pm</option>
      <option value="3:15pm">3:15pm</option>
      <option value="3:30pm">3:30pm</option>
      <option value="3:45pm">3:45pm</option>
      <option value="4:00pm">4:00pm</option>
    </field>

   	<!-- state -->
   	<field
      class="inputbox"
      default="-1"
      description="JFIELD_PUBLISHED_DESC"
      label="JSTATUS"
      name="state"
      size="1"
      type="list">
        <option value="1">COM_SERVICES_STATUS_COMPLETE</option>
        <option value="0">COM_SERVICES_STATUS_SCHEDULED</option>
        <option value="-1">COM_SERVICES_STATUS_PENDING</option>
      	<option value="-2">COM_SERVICES_STATUS_DENIED</option>
    </field>
    
    <!-- sub_title -->
    <field
      description="COM_SERVICES_FORM_WEBINAR_SUB_TITLE_DESC"
      label="COM_SERVICES_FORM_WEBINAR_SUB_TITLE_LBL"
      maxlength="150"
      name="sub_title"
      type="text" />
    
    <!-- title -->
    <field
      description="COM_SERVICES_FORM_WEBINAR_TITLE_DESC"
      label="COM_SERVICES_FORM_WEBINAR_TITLE_LBL"
      maxlength="150"
      name="title"
      type="text" />

    <!-- webinar_id -->
    <field
      label="Webinar ID"
      name="webinar_id"
      type="text" />

	</fieldset>
</form>