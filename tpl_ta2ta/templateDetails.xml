<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.6/template-install.dtd">
<extension version="3.1" type="template" client="site" method="upgrade">
	<name>ta2ta</name>
	<creationDate>25 April 2013</creationDate>
	<author>Zachary Draper</author>
	<authorEmail>zdraper@ncjfcj.org</authorEmail>
	<authorUrl>http://www.ta2ta.org</authorUrl>
	<copyright>Copyright (C) 2013 NCJFCJ. All rights reserved.</copyright>
	<license></license>
	<version>2.1.1</version>
	<description>Technical Assistance to Technical Assistance Providers (TA2TA) Template Created For Joomla! 3.x.</description>

	<files>
		<folder>html</folder>
		<folder>img</folder>
		<folder>js</folder>
		<folder>language</folder>
		<folder>plugins</folder>
		<folder>styles</folder>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>favicon.ico</filename>
		<filename>index.html</filename>
		<filename>index.php</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>	
		<filename>templateDetails.xml</filename>
	</files>

	<positions>
		<position>banner</position>
		<position>breadcrumbs</position>
		<position>content-bottom</position>
		<position>content-top</position>
		<position>copyright</position>
		<position>disclaimer</position>
		<position>left-menu</position>
		<position>legal-menu</position>
		<position>main-nav</position>
		<position>search</position>
		<position>small-top-nav</position>
	</positions>
	
	<languages>
		<language tag="en-GB">language/en-GB/en-GB.tpl_ta2ta.ini</language>
	</languages>
</extension>